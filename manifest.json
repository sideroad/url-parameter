{
  "manifest_version": 2,

  "name": "URL Parameter",
  "description": "Manipulate URL parameter and submit.",
  "version": "1.8",
  "background": {
    "scripts": [
      "js/background.min.js"
    ]
  },
  "icons": {
    "16": "img/icon_128.png",
    "48": "img/icon_128.png",
    "128": "img/icon_128.png"
  },
  "browser_action": {
    "default_icon": "img/icon.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "web_accessible_resources": [
    "img/glyphicons-halflings.png",
    "img/glyphicons-halflings-white.png"
  ]

}
